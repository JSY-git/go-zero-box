type HelloResponse {
    Code int `json:"code"`
    Message string `json:"message"`
}

type (
    HelloReq {
    }
    HelloResp {
        HelloResponse
        Data HelloData `json:"data"`
    }
    HelloData {
    }
)


@server(
    group: hello
    prefix: /api
    tags: "Hello world"
)
service app {
    @doc "Hello world"
    @handler Hello
    get /hello (HelloReq) returns (HelloResp)
}
